{
  "name": "monorepo-example",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:docs": "turbo run start --filter=docs",
    "build:docs": "turbo run build --filter=docs",
    "dev:web": "turbo run start --filter=web",
    "build:web": "turbo run build --filter=web",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@tsconfig/create-react-app": "1.0.3",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.4"
}